<p id="notice"><%= notice %></p>

<h1>Listing Common Expense Subunits</h1>

<table>
  <thead>
    <tr>
      <th>Subunit</th>
      <th>Electricity charge</th>
      <th>Water charge</th>
      <th>Gas charge</th>
      <th>Others charge</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @common_expense_subunits.each do |common_expense_subunit| %>
      <tr>
        <td><%= common_expense_subunit.subunit_id %></td>
        <td><%= common_expense_subunit.electricity_charge %></td>
        <td><%= common_expense_subunit.water_charge %></td>
        <td><%= common_expense_subunit.gas_charge %></td>
        <td><%= common_expense_subunit.others_charge %></td>
        <td><%= link_to 'Show', common_expense_subunit %></td>
        <td><%= link_to 'Edit', edit_common_expense_subunit_path(common_expense_subunit) %></td>
        <td><%= link_to 'Destroy', common_expense_subunit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Common expense subunit', new_common_expense_subunit_path %>
