<p id="notice"><%= notice %></p>

<h1>Listing Outcomes</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Type</th>
      <th>Property</th>
      <th>Ammount</th>
      <th>Details</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @outcomes.each do |outcome| %>
      <tr>
        <td><%= outcome.date %></td>
        <td><%= outcome.outcome_category.name %></td>
        <td><%= outcome.property.address %></td>
        <td><%= outcome.ammount %></td>
        <td><%= outcome.details %></td>
        <td><%= link_to 'Show', outcome %></td>
        <td><%= link_to 'Edit', edit_outcome_path(outcome) %></td>
        <td><%= link_to 'Destroy', outcome, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Outcome', new_outcome_path %>
