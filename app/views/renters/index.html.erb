<p id="notice"><%= notice %></p>

<div class='container-fluid' id="holder-properties-list">
  <div class="col-lg-12 col-md-4">
      <div class="holder-extern-no-verticality">
        <div class="table-responsive">
            <table class="table">
                <thead>
                    <tr>
                        <th class="table-header">NOMBRE</th>
                        <th class="table-header">EMAIL</th>
                        <th colspan="3"></th>
                    </tr>
                </thead>

                <tbody>
                    <% @renters.each do |r| %>
                    <tr>
                        <td class="table-data"><%= r.renter_name_lastname %></td>
                        <td class="table-data"><%= r.email %></td>
                        <td class="table-data"><%= link_to 'Edit', edit_renter_registration_path(r) %></td>
                        <td class="table-data"><%= link_to 'Destroy', cancel_renter_registration_path(r), method: :delete, data: { confirm: 'Are you sure?' } %></td>
                    </tr>
                    <% end %>
                </tbody>
            </table>
        </div>
      </div>
  </div>
</div>
